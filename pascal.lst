      %
11343: OrdinalLow( Node theType ) >> int:
11345:    [ oNodeType( theType )
11352:       | nIntegerType :  >> oMININT
11355:       | nBooleanType :  >> 0
11360:       | nCharType :     >> 0
11365:       | nEnumType :
11367:          Node enumScope = oNodeGet( theType, qScope )
11380:          Node first = oNodeIterValue( oNodeGetIter( enumScope, qDecls ) )
11397:          >> oNodeGetInt( first, qValue )
11407:       | nSubrangeType : >> oNodeGetInt( theType, qLow )
11419:       | * :             #eNotOrdinalType
11434:                         >> 0
11437:    ];
      
      
