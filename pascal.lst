10841:    [
10841:       | '(' :
10843:          {
10843:             @Expr
10845:             [ oTypeSNodeType
10847:                | nIntegerType :             .tWriteI
10850:                | nBooleanType :             .tWriteBool
10854:                | nByteType :                .tCastBtoI  .tWriteI
10860:                | nCharType :                .tWriteChar
