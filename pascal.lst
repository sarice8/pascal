9502:       | true :
               % parameter is integer
               % result is char
9503:          '('
9505:          @Expr
9507:          [ oTypeSNodeType
9509:             | nIntegerType :    .tCastItoB
9512:             | nByteType :
9514:             | * :  #eTypeMismatch
9523:          ]
9523:          oTypeSPop
9524:          oTypeSPush( CharType )
9530:          ')'
9532:          >>
9533:       | * :
9538:    ]
      
