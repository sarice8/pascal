2675:    [ nested
2678:       | true :
2679:          int offset = oScopeAllocType( @PointerTypeTo( IntegerType ) )   % arbitrary ptr type
2694:       | * :
2699:    ]
      
2699:    @FormalArgDecl
2701:    oNodeSet( decl, qParams, paramScope )
      
2713:    ':'
      
2715:    Node theType
2715:    @TypeRef( theType )
2722:    oNodeSet( decl, qType, theType )
      
