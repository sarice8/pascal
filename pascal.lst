      % It must be implicitly convertable to integer.
      % Pop the type stack.
      %
11104: PromoteToIntPop:
11106:    [ oTypeSNodeType
11108:       | nIntegerType :
11109:       | nByteType :        .tCastBtoI
11113:       | * :                #eNotInteger
11122:    ]
11122:    oTypeSPop;
      
      % Convert the top value to integer (if it isn't already).
      % It must be implicitly convertable to integer.
      % Leave integer on the type stack.
      %
11124: PromoteToInt:
11126:    [ oTypeSNodeType
11128:       | nIntegerType :
11129:       | nByteType :        .tCastBtoI
11133:                            oTypeSPop
11134:                            oTypeSPush( IntegerType )
11140:       | * :                #eNotInteger
11149:    ];
      
      
