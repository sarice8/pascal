4555:    Node outerScope = oScopeCurrent
4560:    oScopeBegin( -1, allocUp )   % level is meaningless, and we won't use allocate
      
4569:    {
            % TO DO: assert that name not already seen in this scope, i.e. this enum.
            %  And possibly not in the outerScope either, since that probably wouldn't count as shadowing.
            %  It might be in some other higher/used scope.
4569:       pIdent
4571:       Node decl = @newIdent( nEnumValue, LAST_ID )
4584:       Node decl2 = @newIdent( nEnumValue, LAST_ID )
4597:       oNodeSet( decl, qType, resultType )
4609:       oNodeSet( decl2, qType, resultType )
      
            % build up name table, for i/o
4621:       int nameOffset = oStringAllocLit( ID_STRING( LAST_ID ) )
4634:       oNodeSetInt( decl, qNameOffset, nameOffset )
4646:       oNodeSetInt( decl2, qNameOffset, nameOffset )
4658:       [
4658:          | '=', ':=' :
4660:             @ConstExpr
                  % value must be ascending
                  % this obviously doesn't apply to the first value
4662:             [ equal_zero( numValues )
4669:                | false :
4670:                   [ greater( oValueTop, value )
4679:                      | false :   #eEnumValueNotAscending
4682:                      | * :
4687:                   ]
4687:                | * :
4692:             ]
4692:             [ equal( value, oValueTop )
4701:                | false :
                        % The values differ from 0..N-1.  pred/succ may not be used with this type.
4702:                   oNodeSetBoolean( resultType, qHasGap, true )
4714:                | * :
4719:             ]
4719:             value = oValueTop
4724:             oValuePop
4725:          | * :
4732:       ]
4732:       oNodeSetInt( decl, qValue, value )
4744:       oNodeSetInt( decl2, qValue, value )
4756:       oScopeDeclare( decl )
      
4762:       oScopeEnter( outerScope )
4768:       oScopeDeclare( decl2 )
4774:       oScopeEnd
      
4775:       inc( value )
4781:       inc( numValues )
4787:       [
4787:          | ',' :
4789:          | * :    >
4796:       ]
4796:    }
4798:    ')'
      
4800:    oNodeSet( resultType, qScope, oScopeCurrent )
4811:    oNodeSetInt( resultType, qSize, 4 )   % always integer sized
4823:    oScopeEnd
      
         % Finish creation of the name table
         %    struct {
         %      int    value;
         %      int    padding;
         %      char*  name;
         %    } table[ numValues + 1 ];
         %  final entry will have name == nullptr
         %
         % I'll need to generate init code, to store pointers in global data,
         % since I don't have a relocation mechanism for static data at the moment.
4824:    oCodePush( @GetOrCreateInitCode( globalScope ) )
4835:    oScopeEnter( globalScope )
4841:    int size = multiply( add( numValues, 1 ), 16 )
4861:    int addr = oScopeAlloc( size, 8 )
4874:    oScopeEnd
4875:    oNodeSetInt( resultType, qNameTable, addr )
         % loop over enum values
4887:    NodeIter it = oNodeGetIter( oNodeGet( resultType, qScope ), qDecls )
4907:    {
4907:       Node enumValue = oNodeIterValue( it )
4917:       [ oNodeNull( enumValue )
4924:          | true :  >
4927:          | * :
4932:       ]
4932:       .tPushAddrGlobal  oEmitInt( addr )
4940:       .tPushConstI  oEmitInt( oNodeGetInt( enumValue, qValue ) )
4955:       .tAssignI
4957:       addr = add( addr, 8 )
4970:       .tPushAddrGlobal  oEmitInt( addr )
4978:       .tPushAddrGlobal  oEmitInt( oNodeGetInt( enumValue, qNameOffset ) )
4993:       .tAssignP
4995:       addr = add( addr, 8 )
5008:       oNodeIterNext( it )
5014:    }
         % final table entry
5016:    .tPushAddrGlobal  oEmitInt( addr )
5024:    .tPushConstI  oEmitInt( 0 )
5032:    .tAssignI
5034:    addr = add( addr, 8 )
5047:    .tPushAddrGlobal  oEmitInt( addr )
5055:    .tPushConstI  oEmitInt( 0 )
5063:    .tAssignP
5065:    addr = add( addr, 8 )
5078:    oCodePop
      
5079:    oTypeAdd( resultType )
5086:    ;
      
      
      % --------------------------- Const Expressions --------------------------
      
      % Constant expressions: currently, only literal integers or previously
      % named constant integers are supported, possibly with a negation.
      % Constant expressions are evaluated at compile time.
      
5086: ConstExpr:
5088:    [
5088:       | pIntLit :
5090:          oValuePush( TOKEN_VALUE )
5095:       | pIdent :
5097:          Node decl = oScopeFindRequire
5102:          @ResolveUnitRef( decl )
5109:          [ oNodeType( decl )
5116:             | nConst :
5117:                oValuePush( oNodeGetInt( decl, qValue ) )
5130:             | * :
5135:                #eNotConst
5137:                oValuePush( 0 )
5143:          ]
5143:       | pMinus :
5145:          @ConstExpr
5147:          oValueNegate
5148:    ];
      
      
      
      % ------------------------------ Expressions -----------------------------
      
      % There are two wrappers leading into expression parsing:
      %   Expr()
      %   BooleanExprControlFlow( out Label falseLabel )
      %
      % Expr always produces a value on the runtime expr stack,
      % and pushes its type on the type stack.
      %
      % BooleanExprControlFlow always generates control-flow code.
      % It does not push a value on the runtime expr stack.
      % It does not push a type on the type stack.
      % The control flow code ends up at Here if the expr is true,
      % and jumps to falseLabel if the expr is false.
      % This rule allocates falseLabel, and passes it back.
      % The caller will need to define its location.
      %
      % The internal methods of expr parsing may produce a mix of
      % values and control flow, depending on the operators seen.
      % We don't always do one or the other, hoping to be efficient for
      % cases like   "func_call( bool_var1, bool_var2 )"
      % as well as  "if bool_expr1 or bool_expr2 then ...".
      % If a subexpression rule produces control flow, it will allocate a
      % falseLabel and pass it back up.  And it will push the
      % result type as BooleanCFType, to distinguish from BooleanType
      % which corresponds with a boolean value on the expr stack.
      % 
      % Note that my expression parsing is broken into more levels than
      % grammars describe for Pascal precedence levels.  This follows my
      % original parser implementation.  I think my rationale was that
      % operators from the levels that I split up couldn't have been
      % mixed anyway, since they require different data types
      % (e.g. boolean vs int/real).  And splitting them up simplified
      % my code.  But, I should revisit this.
      %
      
      
      % Public entry point:
      %
      % Parse an expression, leaving a value on the runtime expr stack
      % and its type on the type stack.
      %
      % This rule does not export control flow (though it may generate
      % control flow internally for subexpressions).
      %
5159: Expr:
5161:    Label falseLabel = labelNull
      
5167:    @ExprAllowCF( falseLabel )
5174:    @CFToVal( falseLabel );
      
      
      % Public entry point:
      %
      % Parse a Boolean expression, producing control flow code rather than a value.
      % Does not leave a value on the runtime expr stack.
      % Does not leave a type on the type stack.
      %
      % If the expression is true, the code will fall through to the end (Here).
      % If the expression is false, the code will jump to the given falseLabel,
      % which this rule will allocate but not define the location for.
      % The caller must do so.
      %
5182: BooleanExprControlFlow( out Label falseLabel ):
5184:    @ExprAllowCF( falseLabel )
5191:    [ oTypeSNodeType
5193:       | nBooleanCFType :
5194:       | nBooleanType :
               % convert value to control flow
5196:          falseLabel = oLabelNew
5201:          .tJumpFalse  oEmitLabel( falseLabel )
5209:       | * :
5216:          #eNotBoolean
5218:    ]
5218:    oTypeSPop;
      
      
      % If the current expr result is control flow, convert it to a value.
      % falseLabel is the label that had been used by the control flow.
      % On return it will be set to labelNull.
      %
5220: CFToVal( inout Label falseLabel ):
5222:    [ oTypeSNodeType
5224:       | nBooleanCFType :
5225:          Label doneLabel = oLabelNew
5230:          .tPushConstI  oEmitInt( 1 )
5238:          .tJump  oEmitLabel( doneLabel )
5246:          .tLabel  oEmitLabel( falseLabel )
5254:          .tPushConstI  oEmitInt( 0 )
5262:          .tLabel  oEmitLabel( doneLabel )
5270:          oTypeSPop
5271:          oTypeSPush( BooleanType )
5277:          falseLabel = labelNull
5283:       | * :
5288:    ];
      
      
      % If the current expr result is boolean value, convert it to control flow.
      % falseLabel will be allocated by this rule and passed back.
      % The caller must define its location.
      %
5289: ValToCF( out Label falseLabel ):
5291:    [ oTypeSNodeType
5293:       | nBooleanType :
5294:          falseLabel = oLabelNew
5299:          .tJumpFalse  oEmitLabel( falseLabel )
5307:          oTypeSPop
5308:          oTypeSPush( BooleanCFType )
5314:       | * :
5319:    ];
      
      
      % Parse an expression.  It may produce a value on the expr stack,
      % or control flow, depending on what's seen in the expression.
      % Leaves the expression type on the type stack.
      %
      % If the expression produces control flow, the expression type will be
      % nBooleanCFType.  In this case, a true value falls through to Here,
      % and a false value jumps to falseLabel.  This rule will allocate falseLabel
      % and return it, but will not define its location; the caller must do so.
      %
      % If the expression produces a value, the value will be left on the
      % expression stack, and the type will be some type other than nBooleanCFType.
      % In this case, falseLabel is ignored.
      %
5320: ExprAllowCF( out Label falseLabel ):
5322:    @BoolExprAllowCF( falseLabel )
5329:    {[
5329:       | '=' :
5331:          @CFToVal( falseLabel )
5338:          @PromoteToIntOptional
5340:          @BoolExprAllowCF( falseLabel )
5347:          @CFToVal( falseLabel )
5354:          @PromoteToIntOptional
5356:          @MatchTypes
5358:          [ oTypeSNodeType
5360:             | nBooleanType, nCharType :     .tEqualB
5363:             | nIntegerType, nEnumType :     .tEqualI
5367:             | nPointerType, nUniversalPointerType :    .tEqualP
5371:             | nShortStringType, nStrLitType :            #eNotImplemented
5375:             | * :                           #eNotAllowed
5396:          ]
5396:          oTypeSPop
5397:          oTypeSPush( BooleanType )
      
5403:       | '<>' :
5405:          @CFToVal( falseLabel )
5412:          @PromoteToIntOptional
5414:          @BoolExprAllowCF( falseLabel )
5421:          @CFToVal( falseLabel )
5428:          @PromoteToIntOptional
5430:          @MatchTypes
5432:          [ oTypeSNodeType
5434:             | nBooleanType, nCharType :     .tNotEqualB
5437:             | nIntegerType, nEnumType :     .tNotEqualI
5441:             | nPointerType, nUniversalPointerType :    .tNotEqualP
5445:             | nShortStringType, nStrLitType :            #eNotImplemented
5449:             | * :                           #eNotAllowed
5470:          ]
5470:          oTypeSPop
5471:          oTypeSPush( BooleanType )
      
5477:       | '<' :
5479:          @CFToVal( falseLabel )
5486:          @PromoteToIntOptional
5488:          @BoolExprAllowCF( falseLabel )
5495:          @CFToVal( falseLabel )
5502:          @PromoteToIntOptional
5504:          @MatchTypes
5506:          [ oTypeSNodeType
5508:             | nBooleanType, nCharType :     .tLessB
5511:             | nIntegerType, nEnumType :     .tLessI
5515:             | nPointerType, nUniversalPointerType :   .tLessP
5519:             | nShortStringType, nStrLitType :            #eNotImplemented
5523:             | * :                           #eNotAllowed
5544:          ]
5544:          oTypeSPop
5545:          oTypeSPush( BooleanType )
      
5551:       | '>' :
5553:          @CFToVal( falseLabel )
5560:          @PromoteToIntOptional
5562:          @BoolExprAllowCF( falseLabel )
5569:          @CFToVal( falseLabel )
5576:          @PromoteToIntOptional
5578:          @MatchTypes
5580:          [ oTypeSNodeType
5582:             | nBooleanType, nCharType :     .tGreaterB
5585:             | nIntegerType, nEnumType :     .tGreaterI
5589:             | nPointerType, nUniversalPointerType :   .tGreaterP
5593:             | nShortStringType, nStrLitType :            #eNotImplemented
5597:             | * :                           #eNotAllowed
5618:          ]
5618:          oTypeSPop
5619:          oTypeSPush( BooleanType )
      
5625:       | '<=' :
5627:          @CFToVal( falseLabel )
5634:          @PromoteToIntOptional
5636:          @BoolExprAllowCF( falseLabel )
5643:          @CFToVal( falseLabel )
5650:          @PromoteToIntOptional
5652:          @MatchTypes
5654:          [ oTypeSNodeType
5656:             | nBooleanType, nCharType :     .tLessEqualB
5659:             | nIntegerType, nEnumType :     .tLessEqualI
5663:             | nPointerType, nUniversalPointerType :   .tLessEqualP
5667:             | nShortStringType, nStrLitType :            #eNotImplemented
5671:             | * :                           #eNotAllowed
5692:          ]
5692:          oTypeSPop
5693:          oTypeSPush( BooleanType )
      
5699:       | '>=' :
5701:          @CFToVal( falseLabel )
5708:          @PromoteToIntOptional
5710:          @BoolExprAllowCF( falseLabel )
5717:          @CFToVal( falseLabel )
5724:          @PromoteToIntOptional
5726:          @MatchTypes
5728:          [ oTypeSNodeType
5730:             | nBooleanType, nCharType :     .tGreaterEqualB
5733:             | nIntegerType, nEnumType :     .tGreaterEqualI
5737:             | nPointerType, nUniversalPointerType :   .tGreaterEqualP
5741:             | nShortStringType, nStrLitType :            #eNotImplemented
5745:             | * :                           #eNotAllowed
5766:          ]
5766:          oTypeSPop
5767:          oTypeSPush( BooleanType )
      
5773:       | * :
5788:          >
5790:    ]};
      
      
5793: BoolExprAllowCF( out Label falseLabel ):
5795:    Label trueLabel = labelNull
      
5801:    @BoolTermAllowCF( falseLabel )
5808:    {[
5808:       | pOr :
5810:          [ oTypeSNodeType
5812:             | nBooleanCFType :
                     % We are true here. Jump ahead to done
5813:                [ equal_label( trueLabel, labelNull )
5823:                   | true :  trueLabel = oLabelNew
5829:                   | * :
5834:                ]
5834:                .tJump  oEmitLabel( trueLabel )
5842:             | nBooleanType :
5844:                @ValToCF( falseLabel )
                     % We are true here. Jump ahead to done
5851:                [ equal_label( trueLabel, labelNull )
5861:                   | true :  trueLabel = oLabelNew
5867:                   | * :
5872:                ]
5872:                .tJump  oEmitLabel( trueLabel )
5880:             | * : #eNotBoolean
5889:          ]
5889:          oTypeSPop
      
               % false in previous term will jump here, to try this alternative term
5890:          .tLabel  oEmitLabel( falseLabel )
      
               % we'll get a new falseLabel for this term
5898:          falseLabel = labelNull
5904:          @BoolTermAllowCF( falseLabel )
      
5911:          [ oTypeSNodeType
5913:             | nBooleanCFType :
5914:             | nBooleanType :
5916:                @ValToCF( falseLabel )
5923:             | * : #eNotBoolean
5932:          ]
      
5932:          oTypeSPop
5933:          oTypeSPush( BooleanCFType )
      
5939:       | * :
5944:          >
5946:    ]}
      
         % any short-circuit trues jump here to the end
5948:    [ equal_label( trueLabel, labelNull )
5958:       | false :
5959:          .tLabel  oEmitLabel( trueLabel )
5967:       | * :
5972:    ]
5973:    ;
      
      
5973: BoolTermAllowCF( out Label falseLabel ):
5975:    Label overallFalseLabel = labelNull
      
5981:    @BoolFactorAllowCF( falseLabel )
5988:    {[
5988:       | pAnd :
5990:          [ oTypeSNodeType
5992:             | nBooleanCFType :
5993:             | nBooleanType :
5995:                @ValToCF( falseLabel )
6002:             | * :
6009:                #eNotBoolean
6011:          ]
6011:          oTypeSPop
      
               % We're going to have a different falseLabel for each term,
               % because we let each rule allocate its own (rather than accepting labels passed in).
               % And in this case we want them all to go to the same place,
               % to be determined by the caller.
               %
               % I'll allow for label aliases.  This will be more optimal than generating
               % multiple private false-handling code segments, that the true path would have to
               % jump around.  That would be hard to eliminate afterwards.
      
6012:          [ equal_label( overallFalseLabel, labelNull )
6022:             | true :
6023:                overallFalseLabel = oLabelNew
6028:                .tLabelAlias  oEmitLabel( falseLabel )  oEmitLabel( overallFalseLabel )
6042:                falseLabel = overallFalseLabel
6048:             | * :
6053:          ]
      
6053:          Label factorFalseLabel = labelNull
6059:          @BoolFactorAllowCF( factorFalseLabel )
      
6066:          [ oTypeSNodeType
6068:             | nBooleanCFType :
6069:             | nBooleanType :
6071:                @ValToCF( factorFalseLabel )
6078:             | * : #eNotBoolean
6087:          ]
6087:         .tLabelAlias  oEmitLabel( factorFalseLabel )  oEmitLabel( overallFalseLabel )
      
6101:       | * :
6106:          >
6108:    ]};
      
6111: BoolFactorAllowCF( out Label falseLabel ):
6113:    [
6113:       | pNot :
6115:          Label factorFalseLabel = labelNull
      
6121:          @BoolFactorAllowCF( factorFalseLabel )
6128:          [ oTypeSNodeType
6130:             | nBooleanCFType :
                     % We have control flow, and are sitting at the point that has value false.
6131:                falseLabel = oLabelNew
6136:                .tJump  oEmitLabel( falseLabel )
                     % The true path jumps here
6144:                .tLabel  oEmitLabel( factorFalseLabel )
      
6152:             | nBooleanType :
                     % I have a value.  I'll leave it as a value, rather than create control flow.
                     % See how this goes.
6154:                .tNot
      
6156:             | * : #eNotBoolean
6165:          ]
      
6165:       | * :
6170:          @ArithExprAllowCF( falseLabel )
6177:    ];
      
6178: ArithExprAllowCF( out Label falseLabel ):
6180:    boolean first = true
6186:    int tempStrOffset
      
6186:    @TermAllowCF( falseLabel )
6193:    {[
6193:       | '+' :
6195:          [ oTypeSNodeType
6197:             | nIntegerType, nByteType :
6198:                @PromoteToIntPop
6200:                @TermAllowCF( falseLabel )  % actually not allowing CF, will be caught by type check
6207:                @PromoteToInt
6209:                .tAddI
      
6211:             | nShortStringType, nStrLitType, nCharType :
                     % String concatenation
6213:                [ first
6216:                   | true :  % move into a temp string, so subsequent term(s) can further modify it.
6217:                             tempStrOffset = @MoveIntoTempShortString
6223:                             first = false
6229:                   | * :
6234:                ]
6234:                @TermAllowCF( falseLabel )  % actually not allowing CF, will be caught by type check
6241:                [ oTypeSNodeType
6243:                   | nShortStringType, nStrLitType :
6244:                      @ShortStringAppendShortString
                           % Have to re-push the temp string.
                           % If we ever have .tDup then the append method could do it and we wouldn't need this
6246:                      .tPushAddrLocal  oEmitInt( tempStrOffset )
6254:                      oTypeSPush( ShortStringType )
6260:                   | nCharType :
6262:                      @ShortStringAppendChar
                           % Have to re-push the temp string.
                           % If we ever have .tDup then the append method could do it and we wouldn't need this
6264:                      .tPushAddrLocal  oEmitInt( tempStrOffset )
6272:                      oTypeSPush( ShortStringType )
6278:                   | * :
6287:                      #eTypeMismatch
6289:                      oTypeSPop
6290:                ]
      
6290:             | nPointerType, nUniversalPointerType :
6292:                Node elementType = oNodeGet( oTypeSTop, qBaseType )
6304:                int elementSize = oNodeGetInt( elementType, qSize )
6317:                @TermAllowCF( falseLabel )  % actually not allowing CF
6324:                @PromoteToIntPop
6326:                [ equal( elementSize, 1 )
6336:                   | false :
6337:                      .tPushConstI  oEmitInt( elementSize )
6345:                      .tMultI
6347:                   | * :
6352:                ]
6352:                .tAddPI
6354:             | * :
6371:                #eNotAllowed
6373:          ]
      
6373:       | '-' :
               % TO DO: pointer arithmetic, as above
      
6375:          @PromoteToIntPop
6377:          @TermAllowCF( falseLabel )
6384:          @PromoteToInt
6386:          .tSubI
6388:       | * :
6395:          >
6397:    ]};
      
6400: TermAllowCF( out Label falseLabel ):
6402:    @FactorAllowCF( falseLabel )
6409:    {[
6409:       | pTimes :
6411:          @PromoteToIntPop
6413:          @FactorAllowCF( falseLabel )  % actually not allowing CF, will be caught by type check
6420:          @PromoteToInt
6422:          .tMultI
6424:       | pDivide :
6426:          @PromoteToIntPop
6428:          @FactorAllowCF( falseLabel )
6435:          @PromoteToInt
6437:          .tDivI
6439:       | * :
6446:          >
6448:    ]};
      
6451: FactorAllowCF( out Label falseLabel ):
6453:    [
6453:       | pPlus :
6455:          @PrimaryAllowCF( falseLabel )  % actually not allowing CF, will be caught by type check
               % TO DO: unary +/- is supposed to leave expression type as-is.
6462:          @PromoteToInt
6464:       | pMinus :
6466:          @PrimaryAllowCF( falseLabel )
               % TO DO: unary +/- is supposed to leave expression type as-is.
6473:          @PromoteToInt
6475:          .tNegI
6477:       | * :
6484:          @PrimaryAllowCF( falseLabel )
6491:    ];
      
6492: PrimaryAllowCF( out Label falseLabel ):
6494:    [
6494:       | pIntLit :
6496:          .tPushConstI  oEmitInt( TOKEN_VALUE )
6503:          oTypeSPush( IntegerType )
      
6509:       | pCharLit :
6511:          .tPushConstI  oEmitInt( TOKEN_VALUE )
6518:          oTypeSPush( CharType )
      
6524:       | '(' :
6526:          @ExprAllowCF( falseLabel )
6533:          ')'
      
6535:       | pStrLit :
6537:          int addr = oStringAllocShortStringLit( CURRENT_STRLIT )     % store in global data
6546:          .tPushAddrGlobal oEmitInt( addr )
               % TO DO: consider defining a ShortString type with the exact capacity used.
               % But, I don't think it should matter here, since nobody should be assigning to this string.
6554:          oTypeSPush( StrLitType )
               % This seems unlikely, but the strlit may be followed by
               % array indexing.  Handle this the same way we would with a variable expression (see VarExpr)
               % First, advance the pointer by index if any
6560:          @LValueIndexes
               % get final value of dereferencing, if any
6562:          @FetchVar
      
6564:       | pIdent :
6566:          Node decl = oScopeFindRequire
6571:          @ResolveUnitRef( decl )
6578:          Node theType
      
6578:          [ oNodeType( decl )
6585:             | nFunc :
6586:                @Call( decl )
      
6593:             | nBuiltInFunc :
6595:                @CallBuiltInFunc( decl )
      
6602:             | nConst, nEnumValue :
6604:                theType = oNodeGet( decl, qType )
6617:                oTypeSPush( theType )
6623:                [ oTypeSNodeType
6625:                   | nIntegerType, nEnumType, nBooleanType :
6626:                      .tPushConstI @EmitValue( decl )
6635:                   | * :
6644:                      #eNotImplemented
6646:                ]
      
6646:             | nGlobalVar, nLocalVar, nParam, nTypedConst :
6648:                @VarExpr( decl )
      
6655:             | * :
6674:                #eNotValue
6676:                oTypeSPush( IntegerType )
6682:          ]
      
6682:       | '@' :        % @var -- pointer to var
6684:          pIdent
      
6686:          Node decl = oScopeFindRequire
6691:          @ResolveUnitRef( decl )
6698:          Node theType
      
               % writeable is false because we're not on the left hand side of an assignment,
               % or passign into a var parameter.  So it's ok to take the address of a typed const.
6698:          @LValueVar( decl, false )
      
               % The above leaves the pointed-to type on type stack.
               % Replace with ptr-to-<type>.
6708:          theType = oTypeSTop
6713:          oTypeSPop
6714:          Node ptrType = @PointerTypeTo( theType )
6725:          oTypeSPush( ptrType )
      
6731:       | * :
6746:          #eNotValue
6748:          oTypeSPush( IntegerType )
6754:    ];
      
      
      % Parse a variable expression, given the declaration of the
      % just-accepted identifier.
      % Leaves the value on the expression stack, and type on the type stack.
      %
6755: VarExpr( Node decl ):
6757:    Node theType = oNodeGet( decl, qType )
         % how many levels up the static scope? Will ignore for globals.
6770:    int uplevels = @DeclUpLevels( decl )
      
6781:    oTypeSPush( theType )
6787:    [ oTypeSNodeType
6789:       | nIntegerType, nEnumType :
6790:          [ oNodeType( decl )
6797:             | nGlobalVar, nTypedConst :   .tPushGlobalI  @EmitValue( decl )
6807:             | nLocalVar :
6809:                [ equal_zero( uplevels )
6816:                   | true :  .tPushLocalI  @EmitValue( decl )
6826:                   | * :     .tPushUpLocalI  oEmitInt( uplevels )  @EmitValue( decl )
6846:                ]
6846:             | nParam :
6848:                [ oNodeGetBoolean( decl, qInOut )
6858:                   | true :    % VAR param points to the var.  Auto dereference.
6859:                      [ equal_zero( uplevels )
6866:                         | true :  .tPushParamP  @EmitValue( decl )
6876:                         | * :     .tPushUpParamP  oEmitInt( uplevels )  @EmitValue( decl )
6896:                      ]
6896:                      .tFetchI
6898:                   | * :
6903:                      [ equal_zero( uplevels )
6910:                         | true :  .tPushParamI  @EmitValue( decl )
6920:                         | * :     .tPushUpParamI  oEmitInt( uplevels )  @EmitValue( decl )
6940:                      ]
6940:                ]
6940:          ]
      
6952:       | nBooleanType, nByteType, nCharType :
6954:          [ oNodeType( decl )
6961:             | nGlobalVar, nTypedConst :   .tPushGlobalB  @EmitValue( decl )
6971:             | nLocalVar :
6973:                [ equal_zero( uplevels )
6980:                   | true :  .tPushLocalB  @EmitValue( decl )
6990:                   | * :     .tPushUpLocalB  oEmitInt( uplevels )  @EmitValue( decl )
7010:                ]
7010:             | nParam :
7012:                [ oNodeGetBoolean( decl, qInOut )
7022:                   | true :    % VAR param points to the var.  Auto dereference.
7023:                      [ equal_zero( uplevels )
7030:                         | true :  .tPushParamP  @EmitValue( decl )
7040:                         | * :     .tPushUpParamP  oEmitInt( uplevels )  @EmitValue( decl )
7060:                      ]
7060:                      .tFetchB
7062:                   | * :
7067:                      [ equal_zero( uplevels )
7074:                         | true :  .tPushParamB  @EmitValue( decl )
7084:                         | * :     .tPushUpParamB  oEmitInt( uplevels )  @EmitValue( decl )
7104:                      ]
7104:                ]
7104:          ]
      
7116:       | nFileType :
7118:          #eNotImplemented
      
7120:       | nPointerType, nUniversalPointerType :
7122:          [ oNodeType( decl )
7129:             | nGlobalVar, nTypedConst :   .tPushGlobalP @EmitValue( decl )
7139:             | nLocalVar :
7141:                [ equal_zero( uplevels )
7148:                   | true :  .tPushLocalP  @EmitValue( decl )
7158:                   | * :     .tPushUpLocalP  oEmitInt( uplevels )  @EmitValue( decl )
7178:                ]
7178:             | nParam :
7180:                [ oNodeGetBoolean( decl, qInOut )
7190:                   | true :    % VAR param points to the var.  Auto dereference.
7191:                      [ equal_zero( uplevels )
7198:                         | true :  .tPushParamP  @EmitValue( decl )
7208:                         | * :     .tPushUpParamP  oEmitInt( uplevels )  @EmitValue( decl )
7228:                      ]
7228:                      .tFetchP
7230:                   | * :
7235:                      [ equal_zero( uplevels )
7242:                         | true :  .tPushParamP  @EmitValue( decl )
7252:                         | * :     .tPushUpParamP  oEmitInt( uplevels )  @EmitValue( decl )
7272:                      ]
7272:                ]               
7272:          ]
7284:          [
7284:             | '^' :             % dereferenced
7286:                oTypeSPop
7287:                oTypeSPush( oNodeGet( theType, qBaseType ) )
7300:                @LValueIndexes
7302:                @FetchVar
7304:             | '[' :             % dereferencing pointer like an array [0..] of baseType
7306:                @PointerArraySubscript
                     % modify addr for any subsequent subscripts, field references, etc
7308:                @LValueIndexes
7310:                @FetchVar
7312:             | * :               % just ptr value alone
7319:          ]
      
7319:       | nArrayType, nRecordType, nShortStringType :
               % compound type
               % first, push addr of start of var
7321:          [ oNodeType( decl )
7328:             | nGlobalVar, nTypedConst :   .tPushAddrGlobal @EmitValue( decl )
7338:             | nLocalVar :
7340:                [ equal_zero( uplevels )
7347:                   | true :  .tPushAddrLocal  @EmitValue( decl )
7357:                   | * :     .tPushAddrUpLocal  oEmitInt( uplevels )  @EmitValue( decl )
7377:                ]
7377:             | nParam :
7379:                [ oNodeGetBoolean( decl, qInOut )
7389:                   | true :    % VAR param points to the var.  Auto dereference.
7390:                      [ equal_zero( uplevels )
7397:                         | true :  .tPushParamP  @EmitValue( decl )
7407:                         | * :     .tPushUpParamP  oEmitInt( uplevels )  @EmitValue( decl )
7427:                      ]
7427:                   | * :
7432:                      [ equal_zero( uplevels )
7439:                         | true :  .tPushAddrParam  @EmitValue( decl )
7449:                         | * :     .tPushAddrUpParam  oEmitInt( uplevels )  @EmitValue( decl )
7469:                      ]
7469:                ]
7469:          ]
               % modify addr for subscripts, field references, etc
7481:          @LValueIndexes
               % get final value
7483:          @FetchVar
7485:    ];
      
      
      % Addr of start of var is on expr stack; type is on type stack.
      % Replace expr stack with value of var.
      % (In the case of compound vars, this remains as the addr of the var).
      
7512: FetchVar:
7514:    [ oTypeSNodeType
7516:       | nIntegerType, nEnumType :  .tFetchI
7519:       | nBooleanType, nByteType, nCharType :  .tFetchB
7523:       | nFileType :   #eNotImplemented
7527:       | nPointerType :             .tFetchP
7531:       | nUniversalPointerType :    #eCantDereference
7535:       | nArrayType, nRecordType, nStrLitType, nShortStringType :  % compound type; leave as addr
7537:    ];
      
      
      % Addr of start of var is already on run stack, and type is on type stack.
      % Read subscripts, record fields, etc, to get final addr on run stack, and final type
      % on type stack.
      
7566: LValueIndexes:
7568:    {[
7568:       | '[' :
7570:          [ oTypeSNodeType
7572:             | nArrayType :    @ArraySubscripts
7575:             | nPointerType :  @PointerArraySubscript
7579:             | nStrLitType, nShortStringType :  @ShortStringArraySubscript
7583:             | * :             #eNotArray
7596:          ]
7596:       | '.' :       @RecordFieldRef
7600:       | '^' :       @PointerDeref   % TO DO: I suspect I don't have logic right here
                                          % about precondition and using .tFetchP
                                          % Ideally this should take over for the ^ in VarExpr too.
7604:       | * :         >
7615:    ]};
      
7618: ArraySubscripts:
7620:    [ oTypeSNodeType
7622:       | nArrayType :
7623:       | * :       #eNotArray
7630:    ]
7630:    {
7630:       [ oTypeSNodeType
7632:          | nArrayType :
7633:          | * :    #eTooManySubscripts
7640:       ]
      
            % low subscript of this dimension
7640:       int low = @OrdinalLow( oNodeGet( oTypeSTop, qIndexType ) )
      
            % replace type stack with next dimension, or OF type
7657:       Node baseType
7657:       baseType = oNodeGet( oTypeSTop, qBaseType )
7669:       oTypeSPop
7670:       oTypeSPush( baseType )
      
7676:       @Expr
7678:       @RequireIntPop
            % adjust for low subscript
7680:       [ equal_zero( low )
7687:          | false :
7688:             .tPushConstI oEmitInt( low ) .tSubI
7698:          | * :
7703:       ]
      
            % multiply by element size
7703:       int size = oNodeGetInt( baseType, qSize )
7716:       [ equal( size, 1 )
7726:          | false :
7727:             .tPushConstI oEmitInt( size ) .tMultI
7737:          | * :
7742:       ]
      
            % update start address
7742:       .tAddPI
7744:       [
7744:          | ']' :  >
7748:          | ',' :
7750:       ]
7758:    };
      
      
      % pointer[index] - similar to array[index] where index counts from 0.
      % On entry, the pointer value is on the stack, and pointer type is on the type stack.
      % We've already accepted the '['.
      % On exit, the address of the pointed-to element is on the stack,
      % and the element type is on the type stack.
      %
7761: PointerArraySubscript:
7763:    [ oTypeSNodeType
7765:       | nPointerType :
7766:       | * :    #eCantDereference
7773:    ]
         % replace type stack with base type
7773:    Node baseType = oNodeGet( oTypeSTop, qBaseType )
7785:    oTypeSPop
7786:    oTypeSPush( baseType )
         
7792:    @Expr
7794:    @RequireIntPop
         % multiply by element size
7796:    int size = oNodeGetInt( baseType, qSize )
7809:    [ equal( size, 1 )
7819:       | false :
7820:          .tPushConstI  oEmitInt( size )  .tMultI
7830:       | * :
7835:    ]
         % update start address
7835:    .tAddPI
7837:    ']';
      
      
      % Specialization of array indexing for ShortString.
      % We treat the starting index as 0 so that str[1] sees the first string char,
      % past the length byte.
      %
7840: ShortStringArraySubscript:
7842:    [ oTypeSNodeType
7844:       | nStrLitType, nShortStringType :
7845:       | * :    #eCantDereference
7854:    ]
7854:    oTypeSPop
7855:    oTypeSPush( CharType )
7861:    @Expr
7863:    @RequireIntPop
7865:    .tAddPI
7867:    ']';
      
      
7870: RecordFieldRef:
7872:    [ oTypeSNodeType
7874:       | nRecordType :
7875:       | * :    #eNotRecord
7882:    ]
7882:    oScopeEnter( oNodeGet( oTypeSTop, qScope ) )
7894:    pIdent
7896:    Node field = oScopeFindRequire
         % oScopeFind searches all open scopes, so make sure we found nRecordField
         % which could only be seen in the local scope we entered above.
7901:    [ oNodeType( field )
7908:       | nRecordField :
7909:       | * :   #eNotRecordField
7916:    ]
7916:    oScopeEnd
7917:    int offset = oNodeGetInt( field, qValue )
7930:    [ equal_zero( offset )
7937:       | false :
7938:          .tPushConstI oEmitInt( offset ) .tAddPI
7948:       | * :
7953:    ]
      
         % replace the type on the type stack, with the field type
7953:    oTypeSPop
7954:    oTypeSPush( oNodeGet( field, qType ) )
7968:    ;
      
      
      % ptr deref on left side, i.e. BEFORE addr of ptr var is on stack,
      %                              AFTER addr of thing pointed to is on stack
7968: PointerDeref:
7970:    [ oTypeSNodeType
7972:       | nPointerType :
7973:       | nUniversalPointerType :   #eCantDereference
7977:       | * :       #eNotPointer
7986:    ]
7986:    .tFetchP           % now addr on stack is addr of what we're pointing to
      
7988:    Node theType = oTypeSTop
7993:    oTypeSPop
7994:    oTypeSPush( oNodeGet( theType, qBaseType ) )
8008:    ;
      
      
      % The expr stack contains a value whose type is on top of the type stack.
      % Generate code to coerce the value to the type under it in the type stack
      % (pop the type stack, leaving only the desired type).
      % This is used for assignment.
      %
8008: CoerceType:
8010:    node_type nt = oTypeSNodeType
8015:    oTypeSPop
8016:    [ equal_node_type( nt, oTypeSNodeType )
8025:       | false :
               % Can we implicitly convert the value to the desired type?
8026:          [ oTypeSNodeType
8028:             | nIntegerType :
8029:                [ nt
8032:                   | nByteType :   .tCastBtoI  >>
8036:                   | * :
8041:                ]
8041:             | nByteType :
8043:                [ nt
8046:                   | nIntegerType :   .tCastItoB  >>
8050:                   | * :
8055:                ]
8055:             | nPointerType :
8057:                [ nt
8060:                   | nUniversalPointerType :  >>
8062:                   | nStrLitType :
8064:                      [ equal_node( oTypeSTop, PCharType )
8073:                         | true :
                                 % PChar := 'strlit' i.e. point at strlit[1]
8074:                            .tPushConstI  oEmitInt(1)  .tAddPI  >>
8085:                         | * :
8090:                      ]
8090:                   | * :
8097:                ]
8097:             | nUniversalPointerType :
8099:                [ nt
8102:                   | nPointerType :  >>
8104:                   | * :
8109:                ]
8109:             | nShortStringType :
8111:                [ nt
8114:                   | nStrLitType :  >>
8116:                   | * :
8121:                ]
8121:             | * :
8134:          ]
8134:          #eTypeMismatch
8136:       | * :
8141:    ];
      
      
      % Given a ShortString, StrLit, or Char on the expression stack
      % (and type on the type stack).  Move the value into a new temporary ShortString.
      % Leave that temp string and type on the stacks.
      % Also returns the offset of the temp string.
      %
8142: MoveIntoTempShortString >> int:
8144:    int tempOffset = oScopeAllocType( ShortStringType )
         % copy the value
8154:    [ oTypeSNodeType
8156:       | nShortStringType, nStrLitType :
8157:          .tPushAddrLocal  oEmitInt( tempOffset )
               % put temp addr under value addr
8165:          .tSwap
               % TO DO: we could just copy length + 1, but tCopy takes a fixed #bytes
8167:          .tCopy  oEmitInt( oNodeGetInt( ShortStringType, qSize ) )
8182:       | nCharType :
               % temp[0] = 1
8184:          .tPushAddrLocal  oEmitInt( tempOffset )
8192:          .tPushConstI  oEmitInt( 1 )
8200:          .tAssignB
               % temp[1] = value
8202:          .tPushAddrLocal  oEmitInt( add( tempOffset, 1 ) )
8217:          .tSwap
8219:          .tAssignB
8221:       | * :   #eTypeMismatch
8232:    ]
8232:    oTypeSPop
         % Leave the temp ShortString on the expr stack and type stack
8233:    .tPushAddrLocal  oEmitInt( tempOffset )
8241:    oTypeSPush( ShortStringType )
8247:    >> tempOffset;
      
      
      % Top of expr stack / type stack is ShortString B.
      % Under it is ShortString A.
      % Change A to append the contents of B to it (truncating if necessary).
      % Removes both A and B from the expr stack / type stack.
      %
8251: ShortStringAppendShortString:
         % TO DO: ideally I would do this directly with tcode.  But I think I need some more
         % enhancements for that to work well.   Meanwhile, generate a call to a method.
         % For the moment, I'll implement the method in Pascal, in the mysystem unit.
8253:    .tAllocActuals  oEmitInt( 16 )
8261:    .tPushAddrActual  oEmitInt( 8 )  .tSwap  .tAssignP
8273:    .tPushAddrActual  oEmitInt( 0 )  .tSwap  .tAssignP
8285:    .tCall   @EmitValue( System_ShortStringAppendShortString )
         % TO DO: Ideally I would leave A on the expr stack / type stack.
         %  But I can't really do that without dup, which I hesitate to add.
         %  I'll have the caller push A again themselves.
8294:    .tFreeActuals  oEmitInt( 16 )
8302:    oTypeSPop
8303:    oTypeSPop
8305:    ;
      
      % Top of expr stack / type stack is Char B.
      % Under it is ShortString A.
      % Change A to append char B to it (truncating if necessary).
      % Removes both A and B from the expr stack / type stack.
      %
8305: ShortStringAppendChar:
         % Note at the moment I don't align params
8307:    .tAllocActuals  oEmitInt( 12 )
8315:    .tPushAddrActual  oEmitInt( 8 )  .tSwap  .tAssignI
8327:    .tPushAddrActual  oEmitInt( 0 )  .tSwap  .tAssignP
8339:    .tCall  @EmitValue( System_ShortStringAppendChar )
8348:    .tFreeActuals  oEmitInt( 12 )
8356:    oTypeSPop
8357:    oTypeSPop
8359:    ;
      
      
      % Called on first use of an extern method
      %
8359: DefineExternLabel( Node method ):
         % The extern name might be given explicitly, otherwise the pascal method name.
8361:    int strAddr
8361:    String externalName = oNodeGetString( method, qExternalName )
8374:    [ equal_string( externalName, stringNull )
8384:       | true :
8385:          strAddr = oStringAllocLit( ID_STRING( oNodeGetInt( method, qIdent ) ) )
8406:       | false :
8408:          strAddr = oStringAllocLit( externalName )
8418:    ]
8426:    .tLabelExtern  @EmitValue( method )  oEmitInt( strAddr )
8442:    ;
      
      
      % Parse a call, given an nProc or nFunc for the just-accepted identifier.
      % If a func, this will also generate the code to push the return value on the expr stack,
      % and push the return type on the type stack.
      %
8442: Call( Node method ):
      
         % Prior to first call, issue extern label for a called method.
         % I don't want to issue .tLabelExtern for unused methods, that seems inefficient.
8444:    [ oNodeGetBoolean( method, qExternal )
8454:       | true :
8455:          [ oNodeGetBoolean( method, qUsed )
8465:             | false :
                     % define the extern label on first use
8466:                @DefineExternLabel( method )
8473:             | * :
8478:          ]
8478:      | * :
8483:    ]
8483:    oNodeSetBoolean( method, qUsed, true )
      
         % If the method has a cdecl modifier, the back end will need to use the native
         % calling convention, instead of my own.  But, the convention varies by platform,
         % so the front end won't assume what it needs.  I'll still generate the normal tcode
         % that copies values into actuals space on the stack.
         % However, to help the back end, I will at least use a different instruction for the
         % allocation of the actuals space, and for the actual call.
         %
         % p.s. I only support cdecl on external methods, because I don't have a way
         % to let my own Pascal methods read params passed in with the cdecl calling convention.
         %
8495:    boolean cdecl = oNodeGetBoolean( method, qCdecl )
      
         % If this is a function, we will allocate a temporary
         % in the current scope, to hold the result value.
         % And pass a pointer to that temporary as an extra parameter.
8508:    Node resultType
8508:    int tempOffset
      
8508:    boolean isFunc = equal_node_type( oNodeType( method ), nFunc )
      
8525:    [ isFunc
8528:       | true :
8529:          resultType = oNodeGet( method, qType )
8542:          tempOffset = oScopeAllocType( resultType )
               % TO DO: we could manage temp allocation, so temp space can be reused
               %        by different code branches.
8552:       | * :
8557:    ]
         
      
8557:    Node paramScope = oNodeGet( method, qParams )
8570:    int actualsSize = oNodeGetInt( paramScope, qSize )
8583:    [ cdecl
8586:       | true : .tAllocActualsCdecl  oEmitInt( actualsSize )
8595:       | false :  .tAllocActuals  oEmitInt( actualsSize )
8605:    ]
      
         % If method is nested, it takes a hidden first parameter:
         % a pointer to the frame of the static scope in which the method is declared.
         % The location of a scope's frame is equivalent to offset 0 in the scope's locals space
         % (since local offsets are relative to the scope's frame pointer).
         %
         % The nested method may be in a higher scope (upLevels > 1),
         % or a sibling of the method we're in (upLevels = 1),
         % or an immediate child of the current method (upLevels = 0).
         %
         % To understand the upLevels computation, remember that a method's declaration
         % and its contents (variables, nested functions, and body statement)
         % are in two different scopes.
      
8613:    [ greater( @DeclLevel( method ), 0 )
8628:       | true :
8629:          .tPushAddrActual  oEmitInt( 0 )    % hidden param is at param offset 0
8637:          .tPushAddrUpLocal  oEmitInt( @DeclUpLevels( method ) )  oEmitInt( 0 )
8656:          .tAssignP
8658:       | * :
8663:    ]
      
      
8663:    NodeIter paramIter = oNodeGetIter( paramScope, qDecls )
8676:    Node param = oNodeIterValue( paramIter )
8686:    [
8686:       | '(' :
            
8688:          {
8688:             [ oNodeNull( param )
8695:                | true : >
8698:                | * :
8703:             ]
      
8703:             int offset = oNodeGetInt( param, qValue )
      
                  % Assign actual value to spot in actuals space
8716:             .tPushAddrActual oEmitInt( offset )
8724:             oTypeSPush( oNodeGet( param, qType ) )
      
8737:             [ oNodeGetBoolean( param, qInOut )
8747:                | true :   % A VAR param.
                                % The actual is a pointer to the value.
                                % The type on the type stack (pushed above) is for the pointed-to value.
      
8748:                           @LValueExpr( true )
                                % Addr of the referenced variable is on the expr stack,
                                % type of the variable is on the type stack.
      
                                % Match types more exactly. It's not sufficient to be assign compatible
                                % since the method will be assigning to the actual using the param type.
                                % Maybe something like type contents must match, incl subrange bounds.
8755:                           @MatchTypes
      
8757:                           .tAssignP
      
8759:                | false :  @Expr
8763:                           @CoerceType
                                
                                % Assign to offset in actuals space
      
8765:                           [ oTypeSNodeType
8767:                              | nIntegerType, nEnumType : .tAssignI
8770:                              | nBooleanType, nByteType, nCharType :  .tAssignB
8774:                              | nFileType :   #eNotImplemented
8778:                              | nPointerType, nUniversalPointerType :  .tAssignP
8782:                              | nArrayType, nRecordType, nShortStringType :
                                       % compound types: copy value into actuals space
8784:                                  int size = oNodeGetInt( oTypeSTop, qSize )
8796:                                  .tCopy  oEmitInt( size )    % multi-word copy
8804:                           ]
8830:             ]
8838:             oTypeSPop
      
8839:             oNodeIterNext( paramIter )
8845:             param = oNodeIterValue( paramIter )
8855:             [ oNodeNull( param )
8862:                | true :  >
8865:                | false :
8867:             ]
      
8875:             ','
8877:          }
      
8879:          ')'
      
8881:       | * :
8886:    ]
      
8886:    [ oNodeNull( param )
8893:       | false :    #eMissingParameter
8896:       | * :
8901:    ]
      
8901:    [ isFunc
8904:       | true :
               % Pass result temp as an additional VAR parameter.
8905:          .tPushAddrActual oEmitInt( oNodeGetInt( method, qResultOffset ) )
8920:          .tPushAddrLocal  oEmitInt( tempOffset )
8928:          .tAssignP
8930:       | * :
8935:    ]
      
8935:    [ cdecl
8938:       | true :
8939:          .tCallCdecl  @EmitValue( method )
8948:       | false :
8950:          .tCall   @EmitValue( method )
8959:    ]
      
8967:    [ isFunc
8970:       | true :
               % push return value from temp
8971:          oTypeSPush( resultType )
      
8977:          [ oTypeSNodeType
8979:             | nIntegerType, nEnumType :   .tPushLocalI  oEmitInt( tempOffset )
8988:             | nBooleanType, nByteType, nCharType :   .tPushLocalB  oEmitInt( tempOffset )
8998:             | nFileType :  #eNotImplemented
9002:             | nPointerType, nUniversalPointerType :   .tPushLocalP  oEmitInt( tempOffset )
9012:             | nArrayType, nRecordType, nShortStringType :   % compound type: push address
9014:                                .tPushAddrLocal  oEmitInt( tempOffset )
9022:          ]
9048:       | * :
9053:    ]
         
9053:    .tFreeActuals  oEmitInt( actualsSize )
9062:    ;
      
      
      % Parse a call to a built-in function, identified by the just-accepted identifier.
      % This will also generate the code to push the return value on the expr stack,
      % and push the return type on the type stack.
      %
9062: CallBuiltInFunc( Node method ):
      
         % Ord(x)
9064:    [ oNodeEqual( method, BuiltIn_Ord )
9074:       | true :
               % parameter is any scalar type (including boolean, enum, char)
               % result is integer
9075:          '('
9077:          @Expr
9079:          [ oTypeSNodeType
9081:             | nIntegerType, nEnumType :
9082:             | nBooleanType, nByteType, nCharType :  .tCastBtoI
9086:             | * :  #eTypeMismatch
9101:          ]
9101:          oTypeSPop
9102:          oTypeSPush( IntegerType )
9108:          ')'
9110:          >>
9111:       | * :
9116:    ]
      
         % Chr(x)
9116:    [ oNodeEqual( method, BuiltIn_Chr )
9126:       | true :
               % parameter is integer
               % result is char
9127:          '('
9129:          @Expr
9131:          [ oTypeSNodeType
9133:             | nIntegerType :    .tCastItoB
9136:             | nByteType :
9138:             | * :  #eTypeMismatch
9147:          ]
9147:          oTypeSPop
9148:          oTypeSPush( CharType )
9154:          ')'
9156:          >>
9157:       | * :
9162:    ]
      
         % Pred(x)
9162:    [ oNodeEqual( method, BuiltIn_Pred )
9172:       | true :
               % parameter is any enum type, as long as it has values 0..N-1
               % result is the same enum type
9173:          '('
9175:          @Expr
9177:          [ oTypeSNodeType
9179:             | nEnumType :
                     % TO DO: does this skip past type aliases?
                     % TO DO: what if using pred/succ on a subrange type?
9180:                [ oNodeGetBoolean( oTypeSTop, qHasGap )
9189:                   | true :   #eCantUsePredSuccOnEnumWithValueGaps
9192:                   | * :
9197:                ]
9197:             | * :  #eTypeMismatch
9204:          ]
9204:          .tDecI
9206:          ')'
9208:          >>
9209:       | * :
9214:    ]
      
         % Succ(x)
9214:    [ oNodeEqual( method, BuiltIn_Succ )
9224:       | true :
               % parameter is any enum type, as long as it has values 0..N-1
               % result is the same enum type
9225:          '('
9227:          @Expr
9229:          [ oTypeSNodeType
9231:             | nEnumType :
                     % TO DO: does this skip past type aliases?
                     % TO DO: what if using pred/succ on a subrange type?
9232:                [ oNodeGetBoolean( oTypeSTop, qHasGap )
9241:                   | true :   #eCantUsePredSuccOnEnumWithValueGaps
9244:                   | * :
9249:                ]
9249:             | * :  #eTypeMismatch
9256:          ]
9256:          .tIncI
9258:          ')'
9260:          >>
9261:       | * :
9266:    ]
      
9266:    #eNotImplemented
9269:    ;
      
      
      % ------------------------------ Statements -----------------------------
      
9269: Statement:
9271:    [
9271:       | pWriteln :     @WritelnStmt
9275:       | pWrite :       @WriteStmt
9279:       | pReadln :      @ReadlnStmt
9283:       | pRead :        @ReadStmt
9287:       | pIf :          @IfStmt
9291:       | pWhile :       @WhileStmt
9295:       | pFor :         @ForStmt
9299:       | pRepeat :      @RepeatStmt
9303:       | pBreak :       @BreakStmt
9307:       | pContinue :    @ContinueStmt
9311:       | pBegin :       @BeginStmt
9315:       | pIdent :       @LabelOrAssignOrCallStmt
9319:       | pGoto :        @GotoStmt
9323:       | pIntLit :      % should be an integer label
9325:                        oChangeIntLitToLabelIdent
9326:                        @LabelOrAssignOrCallStmt
9328:       | * :            % null statement : don't accept any tokens
9359:    ];
      
      
9360: LabelOrAssignOrCallStmt:
9362:    Node decl = oScopeFindRequire
9367:    @ResolveUnitRef( decl )
9374:    [ oNodeType( decl )
9381:       | nLabel :                          @LabelDefinition( decl )
9389:                                           @Statement
9391:       | nProc :                           @Call( decl )
9400:       | nGlobalVar, nLocalVar, nParam :   @AssignStmt( decl )
9409:       | nFunc :                           @AssignResultStmt( decl )
9418:       | * :                               #eBadStatement
9435:    ];
      
      
      % Parse a "label:" prefix in front of a statement.
      %
9436: LabelDefinition( Node decl ):
9438:    [ oNodeGetBoolean( decl, qDefined )
9448:       | true :  #eAlreadyDefined
9451:       | * :
9456:    ]
9456:    .tLabel  oEmitLabel( oNodeGetLabel( decl, qValue ) )
9471:    oNodeSetBoolean( decl, qDefined, true )
9483:    ':'
9486:    ;
      
      
      % Given decl, the just-accepted first identifier on the left hand side.
      %
9486: AssignStmt( Node decl ):
      
9488:    @LValueVar( decl, true )
9498:    ':=' 
9500:    @Expr
9502:    @CoerceType
9504:    @Assign
9507:    ;
      
      
      % Performs a raw assignment.
      % Given lvalue 2nd on stack, and expr value top of stack (already coerced to the lvalue type),
      % and type of both of these on the type stack,
      % perform the assignment and pop the type off the type stack.
      %
9507: Assign:
9509:    [ oTypeSNodeType
9511:       | nIntegerType, nEnumType :  .tAssignI
9514:       | nBooleanType, nByteType, nCharType :  .tAssignB
9518:       | nFileType :   #eNotImplemented
9522:       | nPointerType, nUniversalPointerType :  .tAssignP
            % compound var assigns; addrs of src, dest on stack
9526:       | nArrayType, nRecordType, nShortStringType :
                % TO DO: for ShortString, allow destination to be smaller capacity,
                % and truncate the copied value/length.
9528:           int size = oNodeGetInt( oTypeSTop, qSize )
9540:           .tCopy  oEmitInt( size )    % multi-word copy
9548:    ]
9574:    oTypeSPop
9576:    ;
      
      % Given nFunc for the just-accepted identifier on the left hand side.
      % Parse an assignment that represents assignment of the function result.
      %
9576: AssignResultStmt( Node funcDecl ):
         % Only allowed to assign the result of the immediately enclosing function, I think.
9578:    [ oNodeEqual( oNodeGet( funcDecl, qChildScope ), oScopeCurrent )
9594:       | false :   #eNotCurrentFunction
9597:       | * :
9602:    ]
      
         % Result value is stored in the caller's space, via a hidden VAR parameter.
9602:    .tPushParamP  oEmitInt( oNodeGetInt( funcDecl, qResultOffset ) )
9617:    oTypeSPush( oNodeGet( funcDecl, qType ) )
      
9630:    ':='
9632:    @Expr
9634:    @CoerceType
9636:    [ oTypeSNodeType
9638:       | nIntegerType, nEnumType : .tAssignI
9641:       | nBooleanType, nByteType, nCharType :  .tAssignB
9645:       | nFileType :   #eNotImplemented
9649:       | nPointerType, nUniversalPointerType :  .tAssignP
            % compound var assigns; addrs of src, dest on stack
9653:       | nArrayType, nRecordType, nShortStringType :
9655:           int size = oNodeGetInt( oTypeSTop, qSize )
9667:           .tCopy  oEmitInt( size )    % multi-word copy
9675:    ]
9701:    oTypeSPop
9703:    ;
      
      
      
      % Parse an lvalue expression, from scratch.
      % This is an expression that may appear on the left side of an assignment.
      % Leaves the address of the value on the expr stack,
      % and the type of the value at that address on the type stack.
      % If writeable is false, the expression won't be written to, so may access a
      % typed const.
      %
9703: LValueExpr( boolean writeable ):
         % At the moment, all lvalue expressions start with a variable name.
         % I think there can also be typecasts, not supported yet.  Check standard syntax.
9705:    pIdent
9707:    Node decl = oScopeFindRequire
9712:    @ResolveUnitRef( decl )
9719:    [ oNodeType( decl )
9726:       | nGlobalVar, nLocalVar, nParam, nTypedConst :
9727:       | * :  #eNotVar
9740:    ]
9740:    @LValueVar( decl, writeable )
9751:    ;
      
      
      % Parse an lvalue expression (e.g. left side of an assign statement)
      % that starts with the just-accepted identifier indicated by the given decl.
      % Leaves the lvalue address on the expression stack,
      % and the type of the value at that address on the type stack.
      %
9751: LValueVar( Node decl, boolean writeable ):
9753:    [ oNodeType( decl )
9760:       | nGlobalVar :   .tPushAddrGlobal @EmitValue( decl )
9770:       | nLocalVar :    .tPushAddrLocal @EmitValue( decl )
9781:       | nParam :
9783:          [ oNodeGetBoolean( decl, qInOut )
9793:             | true :   % VAR param points to variable.  No dereference.
9794:                        .tPushParamP @EmitValue( decl )
9803:             | * :      .tPushAddrParam @EmitValue( decl )
9817:          ]
9817:       | nTypedConst :
9819:          [ writeable
9822:             | false :  .tPushAddrGlobal @EmitValue( decl )                       
9832:             | * :      #eNotVar
9839:          ]
9839:       | * :            #eNotVar
9852:    ]
      
9852:    oTypeSPush( oNodeGet( decl, qType ) )
      
         % handle subscripts, if any.
         % There is one case that I'm not fitting into the pattern nicely, so need to
         % check for specially here:  if lvalue var is a pointer variable, and is
         % followed by [index], we need to dereference the pointer variable before applying
         % the index.  This is different from handling of [index] on an rvalue expression.
         % Right now I have LValueIndexes working for both lvalue and rvalue (composite types)
         % so can't easily add this behavior in there.
         % TO DO: probably I need separate rules LValueIndexes and RValueIndexes.
         %    This might also explain my confusion about handling of deref operator in LValueIndexes vs rvalue expr,
         %    and having separate rules might clear that up better.
      
9865:    [ oTypeSNodeType
9867:       | nPointerType :
9868:          [
9868:             | '[' :
                     % dereference the pointer var first
9870:                .tFetchP
9872:                @PointerArraySubscript
9874:             | * :
9879:          ]
9879:       | * :
9884:    ]
         % additional subscripts, if any
9884:    @LValueIndexes
9887:    ;
      
      
9887: IncVar( Node decl ):
9889:    @LValueVar( decl, true )
9899:    @RequireIntPop
9901:    @VarExpr( decl )
9908:    oTypeSPop
9909:    .tIncI
9911:    .tAssignI;
      
9914: DecVar( Node decl ):
9916:    @LValueVar( decl, true )
9926:    @RequireIntPop
9928:    @VarExpr( decl )
9935:    oTypeSPop
9936:    .tDecI
9938:    .tAssignI;
      
      
9941: IfStmt:
9943:    Label falseLabel = labelNull
      
9949:    @BooleanExprControlFlow( falseLabel )
9956:    pThen
9958:    @Statement
9960:    [
9960:       | pElse :
9962:          Label doneLabel = oLabelNew
      
9967:          .tJump  oEmitLabel( doneLabel )
9975:          .tLabel oEmitLabel( falseLabel )
9983:          @Statement
9985:          .tLabel oEmitLabel( doneLabel )
      
9993:       | * :
9998:          .tLabel oEmitLabel( falseLabel )
10006:    ];
      
      
10007: ForStmt:
10009:    pIdent
      
10011:    Node decl = oScopeFindRequire
         % TO DO: disallow use of a compound variable field.
         %        Could do that by disallowing use of LValueIndexes,
         %        or by double-checking the decl's qType here.
         % TO DO: I think I read somewhere that the variable must be in current scope.
10016:    @LValueVar( decl, true )
10026:    @RequireIntPop
      
10028:    ':='
      
10030:    @Expr
10032:    @RequireIntPop
10034:    .tAssignI
      
10036:    Label breakLabel = oLabelNew
      
10041:    Label checkLabel = oLabelNew
10046:    .tJump  oEmitLabel( checkLabel )
      
10054:    Label continueLabel = oLabelNew
10059:    .tLabel  oEmitLabel( continueLabel )
10067:    [
10067:       | pTo :
10069:          @IncVar( decl )
10076:          .tLabel  oEmitLabel( checkLabel )
10084:          @VarExpr( decl )  oTypeSPop
10092:          @Expr
10094:          @RequireIntPop
10096:          .tGreaterI
10098:          .tJumpTrue  oEmitLabel( breakLabel )
10106:       | pDownto :
10108:          @DecVar( decl )
10115:          .tLabel  oEmitLabel( checkLabel )
10123:          @VarExpr( decl )  oTypeSPop
10131:          @Expr
10133:          @RequireIntPop
10135:          .tLessI
10137:          .tJumpTrue  oEmitLabel( breakLabel )
10145:    ]
10153:    oLoopPush( continueLabel, breakLabel )
10162:    pDo
10164:    @Statement
10166:    .tJump  oEmitLabel( continueLabel )
10174:    .tLabel  oEmitLabel( breakLabel )
10182:    oLoopPop;
      
      
10184: RepeatStmt:
10186:    Label continueLabel = oLabelNew
10191:    .tLabel  oEmitLabel( continueLabel )
      
10199:    Label breakLabel = oLabelNew
      
10204:    oLoopPush( continueLabel, breakLabel )
10213:    @Statement
10215:    {[
10215:       | ';' :
10217:          @Statement
10219:       | pUntil :
10221:          Label falseLabel
10221:          @BooleanExprControlFlow( falseLabel )
10228:          .tLabelAlias  oEmitLabel( falseLabel )  oEmitLabel( continueLabel )
10242:          >
10244:    ]}
10254:    .tLabel  oEmitLabel( breakLabel )
10262:    oLoopPop;
      
      
10264: WhileStmt:
10266:    Label continueLabel = oLabelNew
10271:    .tLabel  oEmitLabel( continueLabel )
      
10279:    Label breakLabel
10279:    @BooleanExprControlFlow( breakLabel )
      
10286:    oLoopPush( continueLabel, breakLabel )
10295:    pDo
10297:    @Statement
10299:    .tJump  oEmitLabel( continueLabel )
10307:    .tLabel  oEmitLabel( breakLabel )
10315:    oLoopPop;
      
      
10317: ContinueStmt:                     % NOTE, for repeat..until, skips test (??)
10319:    [ equal_label( oLoopContinueLabel, labelNull )
10328:       | true :
10329:          #eNotInALoop
10331:       | false :
10333:          .tJump  oEmitLabel( oLoopContinueLabel )
10340:    ];
      
      
10349: BreakStmt:
10351:    [ equal_label( oLoopBreakLabel, labelNull )
10360:       | true :
10361:          #eNotInALoop
10363:       | false :
10365:          .tJump  oEmitLabel( oLoopBreakLabel )
10372:    ];
      
      
10381: GotoStmt:
10383:    [
10383:       | pIdent :
10385:       | pIntLit :
               % Replace token with identifier "_label_<intlit>"
10387:          oChangeIntLitToLabelIdent
10388:    ]
      
         % Some pascals have rules about where you can jump (to a sibling statement or
         % higher level statement in the block, but not into a lower lower level statement.)
         % fpc just says "jumping into a nested statement can have strange effects."
         % I'm going to follow that and not prohibit any goto, as long as it's in the same block.
      
         % Label must be declared in the same block (same scope) it is used.
         % Some pascals allow non-local goto, but we do not.
         % That would require more tracking of dynamic scope, to unwind stack.
         % Note: in main program statement, this relies on the temporaries scope extending
         %  the main scope (where a label declaration would occur).
10396:    Node decl = oScopeFindRequireInScope( oScopeCurrent )
10405:    oNodeSetBoolean( decl, qUsed, true )
10417:    .tJump  oEmitLabel( oNodeGetLabel( decl, qValue ) )
10433:    ;
      
      
10433: BeginStmt:
10435:    @Statement
10437:    {[
10437:       | ';' :   @Statement
10441:       | pEnd :  >
10445:    ]};
      
      
10456: WritelnStmt:
10458:    @WriteStmt
10460:    .tWriteCR;
      
      
10463: WriteStmt:
10465:    [
10465:       | '(' :
10467:          {
10467:             @Expr
10469:             [ oTypeSNodeType
10471:                | nIntegerType :             .tWriteI
10474:                | nBooleanType :             .tWriteBool
10478:                | nByteType :                .tCastBtoI  .tWriteI
10484:                | nCharType :                .tWriteChar
10488:                | nShortStringType, nStrLitType :   .tWriteShortStr
10492:                | nFileType :                #eNotImplemented
10496:                | nEnumType :
                        % write name via table lookup
10498:                   .tPushAddrGlobal  oEmitInt( oNodeGetInt( oTypeSTop, qNameTable ) )
10512:                   .tWriteEnum
10514:                | nPointerType :
10516:                   [ equal_node( oTypeSTop, PCharType )
10525:                      | true :               .tWritePChar
10528:                      | * :                  .tWriteP
10535:                   ]
10535:                | nUniversalPointerType :    .tWriteP
10539:                | * :                        #eNotAllowed
10564:             ]
10564:             oTypeSPop
10565:             [
10565:                | ')' : >
10569:                | ',' :
10571:             ]
10579:          }
10581:       | * :
10586:    ];
      
      
10587: ReadlnStmt:      % ***
         % TO DO
10590:    ;
      
10590: ReadStmt:
         % TO DO
10593:    ;
      
      % -------------------------------- Utilities -------------------------------
      
      % Return the static scope level of the current scope
      %
10593: ScopeLevel >> int:
10595:    >> oNodeGetInt( oScopeCurrent, qLevel );
      
      % Return the static scope level of the given declaration
      %
10605: DeclLevel( Node decl ) >> int:
10607:    >> oNodeGetInt( oNodeGet( decl, qParentScope ), qLevel );
         
      
      % How many levels higher is the given declaration from the current scope?
      %
10625: DeclUpLevels( Node decl ) >> int:
10627:    >> subtract( @ScopeLevel, @DeclLevel( decl ) );
      
      
      % Require that the top two types on the type stack are matching (ignoring subranges).
      % Pop one of them, leaving only one.
      %
10643: MatchTypes:
10645:    node_type nt = oTypeSNodeType
10650:    oTypeSPop
10651:    [ equal_node_type( nt, oTypeSNodeType )
10660:       | false :
               % Some implicit conversion is allowed even here
10661:          [ oTypeSNodeType
10663:             | nPointerType :
10664:                [ nt
10667:                   | nUniversalPointerType :  >>
10669:                   | * :
10674:                ]
10674:             | nUniversalPointerType :
10676:                [ nt
10679:                   | nPointerType :  >>
10681:                   | * :
10686:                ]
10686:             | * :
10693:          ]
10693:          #eTypeMismatch
10695:       | * :
10700:    ];
      
      
      % Require that the top type on the type stack is integer (ignoring subranges).
      % Pop it.
      %
10701: RequireIntPop:
10703:    [ oTypeSNodeType
10705:       | nIntegerType :
10706:       | * :          #eNotInteger
10713:    ]
10713:    oTypeSPop;
      
10715: RequireInt:
10717:    [ oTypeSNodeType
10719:       | nIntegerType :
10720:       | * :          #eNotInteger
10727:    ];
      
      
      % Convert the top value to integer (if it isn't already).
      % It must be implicitly convertable to integer.
      % Pop the type stack.
      %
10728: PromoteToIntPop:
10730:    [ oTypeSNodeType
10732:       | nIntegerType :
10733:       | nByteType :        .tCastBtoI
10737:       | * :                #eNotInteger
10746:    ]
10746:    oTypeSPop;
      
      % Convert the top value to integer (if it isn't already).
      % It must be implicitly convertable to integer.
      % Leave integer on the type stack.
      %
10748: PromoteToInt:
10750:    [ oTypeSNodeType
10752:       | nIntegerType :
10753:       | nByteType :        .tCastBtoI
10757:                            oTypeSPop
10758:                            oTypeSPush( IntegerType )
10764:       | * :                #eNotInteger
10773:    ];
      
      
      % If the top value is implicitly convertable to integer, do so.
      % Otherwise leave it as-is.
      %
10774: PromoteToIntOptional:
10776:    [ oTypeSNodeType
10778:       | nByteType :        .tCastBtoI
10781:                            oTypeSPop
10782:                            oTypeSPush( IntegerType )
10788:       | * :
10793:    ];
      
      
      
      
10794: RequireBoolPop:
10796:    [ oTypeSNodeType
10798:       | nBooleanType :
10799:       | * :          #eNotBoolean
10806:    ]
10806:    oTypeSPop;
      
10808: RequireBool:
10810:    [ oTypeSNodeType
10812:       | nBooleanType :
10813:       | * :          #eNotBoolean
10820:    ];
      
      
      % Create and return a new nType of the given node_type, adding it to the type table.
      %
10821: newType( node_type nt, int size ) >> Node:
10823:   Node node = oNodeNew( nt )
10833:   oNodeSetInt( node, qSize, size )
10845:   oTypeAdd( node )
10851:   >> node
10855:   ;
      
      
      % Create and return a new nIdent of the given node_type, with the given ident id.
      % Does not add it to any scope.
      %
10855: newIdent( node_type nt, int id ) >> Node:
10857:   Node t = oNodeNew( nt )
10867:   oNodeSetInt( t, qIdent, id )
10879:   >> t
10883:   ;
      
      
      % Return an nType representing a pointer to the given type.
      % (Creates a new type if necessary.)
      %
10883: PointerTypeTo( Node theType ) >> Node:
10885:    Node ptrType = oNodeGet( theType, qPointerType )
10898:    [ oNodeNull( ptrType )
10905:       | true :
10906:          ptrType = oNodeNew( nPointerType )
10916:          oNodeSet( ptrType, qBaseType, theType )
10928:          oNodeSetInt( ptrType, qSize, 8 )
10940:          oTypeAdd( ptrType )
10946:          oNodeSet( theType, qPointerType, ptrType )
10958:       | * :
10963:    ]
10963:    >> ptrType;
      
      
      % Return the low value of an ordinal type
      %
10967: OrdinalLow( Node theType ) >> int:
10969:    [ oNodeType( theType )
10976:       | nIntegerType :  >> oMININT
10979:       | nBooleanType :  >> 0
10984:       | nCharType :     >> 0
10989:       | nEnumType :
10991:          Node enumScope = oNodeGet( theType, qScope )
11004:          Node first = oNodeIterValue( oNodeGetIter( enumScope, qDecls ) )
11021:          >> oNodeGetInt( first, qValue )
11031:       | nSubrangeType : >> oNodeGetInt( theType, qLow )
11043:       | * :             #eNotOrdinalType
11058:                         >> 0
11061:    ];
      
      
      % Return the high value of an ordinal type
      %
11062: OrdinalHigh( Node theType ) >> int:
11064:    [ oNodeType( theType )
11071:       | nIntegerType :  >> oMAXINT
11074:       | nBooleanType :  >> 1
11079:       | nCharType :     >> 255
11084:       | nEnumType :
11086:          Node enumScope = oNodeGet( theType, qScope )
11099:          Node last = oNodeIterValue( oNodeGetIterLast( enumScope, qDecls ) )
11116:          >> oNodeGetInt( last, qValue )
11126:       | nSubrangeType : >> oNodeGetInt( theType, qHigh )
11138:       | * :             #eNotOrdinalType
11153:                         >> 0
11156:    ];
      
      
      % Emit the value in a declaration.
      % For a constant integer, this is the constant value.
      % For a global variable, this is the global address.
      % For a local variable, this is the local address relative to the call stack frame.
      %
11157: EmitValue( Node decl ):
11159:     oEmitInt( oNodeGetInt( decl, qValue ));
      
      
11173: DeclareBuiltInFunc( int id ) >> Node:
11175:    Node decl = @newIdent( nBuiltInFunc, id )
11189:    oScopeDeclare( decl )
11195:    >> decl;
      
      
11199: installBuiltIns:
      
         % "mysystem" is the name of a unit that will be available everywhere.
         % Add its name to the id table so we can easily spot the name later.
11201:    mysystemId = oId_mysystem
      
         % install built-in types
11206:    FileType = @newType( nFileType, 4 )
11220:    IntegerType = @newType( nIntegerType, 4 )
11234:    BooleanType = @newType( nBooleanType, 1 )
11248:    BooleanCFType = @newType( nBooleanCFType, 1 )
11262:    CharType = @newType( nCharType, 1 )
11276:    PCharType = @PointerTypeTo( CharType )
11287:    ByteType = @newType( nByteType, 1 )
         % Universal pointer says base type Byte, but this is only for the sake of
         % pointer arithmetic.  It doesn't matter otherwise, because this type cannot be dereferened.
11301:    UniversalPointerType = @newType( nUniversalPointerType, 8 )
11315:    oNodeSet( UniversalPointerType, qBaseType, ByteType )
         % StrLitType is stored with both a length byte and null termination, so it can be assigned to
         % both a ShortString and a PChar.  StrLits longer than 255 characters appear truncated when
         % referenced as a ShortString, but not when referenced as a PChar.
11327:    StrLitType = @newType( nStrLitType, 256 )
         % ShortString is the original Pascal string type: an array of characters, not null-terminated,
         % with the first byte recording the current length.
         % The default ShortString type has capacity for 255 characters.  The user may define
         % additional ShortString types with smaller capacities.
         % The first character can be accessed as myString[1].
11341:    ShortStringType = @newType( nShortStringType, 256 )
11355:    oNodeSetInt( ShortStringType, qCapacity, 255 )
      
         % Declare those type identifiers.
         % SSL doesn't support strings, so need help from operations e.g. oId_File
      
11367:    Node t
      
11367:    t = @newIdent( nTypeDecl, oId_File )
11380:    oNodeSet( t, qType, FileType )
11392:    oScopeDeclare( t )
      
11398:    t = @newIdent( nTypeDecl, oId_Integer )
11411:    oNodeSet( t, qType, IntegerType )
11423:    oScopeDeclare( t )
      
11429:    t = @newIdent( nTypeDecl, oId_Boolean )
11442:    oNodeSet( t, qType, BooleanType )
11454:    oScopeDeclare( t )
      
11460:    t = @newIdent( nTypeDecl, oId_Char )
11473:    oNodeSet( t, qType, CharType )
11485:    oScopeDeclare( t )
      
11491:    t = @newIdent( nTypeDecl, oId_Byte )
11504:    oNodeSet( t, qType, ByteType )
11516:    oScopeDeclare( t )
      
11522:    t = @newIdent( nTypeDecl, oId_Pointer )
11535:    oNodeSet( t, qType, UniversalPointerType )
11547:    oScopeDeclare( t )
      
11553:    t = @newIdent( nTypeDecl, oId_ShortString )
11566:    oNodeSet( t, qType, ShortStringType )
11578:    oScopeDeclare( t )
      
         % Built-in constants
      
11584:    t = @newIdent( nConst, oId_True )
11597:    oNodeSet( t, qType, BooleanType )
11609:    oNodeSetInt( t, qValue, 1 )
11621:    oScopeDeclare( t )
      
11627:    t = @newIdent( nConst, oId_False )
11640:    oNodeSet( t, qType, BooleanType )
11652:    oNodeSetInt( t, qValue, 0 )
11664:    oScopeDeclare( t )
      
         % Built-in methods
         % These aren't declared like normal methods, because they may have
         % special rules for parameter types (e.g. any scalar type).
         % Calls to these methods will be handled specially by the compiler.
      
