9701:                        oChangeIntLitToLabelIdent
9702:                        @LabelOrAssignOrCallStmt
9704:       | * :            % null statement : don't accept any tokens
9735:    ];
      
      
9736: LabelOrAssignOrCallStmt:
9738:    Node decl = oScopeFindRequire
9743:    @ResolveUnitRef( decl )
9750:    [ oNodeType( decl )
9757:       | nLabel :                          @LabelDefinition( decl )
9765:                                           @Statement
9767:       | nProc :                           @Call( decl )
9776:       | nGlobalVar, nLocalVar, nParam :   @AssignStmt( decl )
9785:       | nFunc :                           @AssignResultStmt( decl )
9794:       | * :                               #eBadStatement
9811:    ];
      
      
      % Parse a "label:" prefix in front of a statement.
      %
9812: LabelDefinition( Node decl ):
9814:    [ oNodeGetBoolean( decl, qDefined )
9824:       | true :  #eAlreadyDefined
9827:       | * :
9832:    ]
9832:    .tLabel  oEmitLabel( oNodeGetLabel( decl, qValue ) )
9847:    oNodeSetBoolean( decl, qDefined, true )
9859:    ':'
9862:    ;
      
      
      % Given decl, the just-accepted first identifier on the left hand side.
      %
9862: AssignStmt( Node decl ):
      
9864:    @LValueVar( decl, true )
9874:    ':=' 
9876:    @Expr
9878:    @CoerceType
9880:    @Assign
9883:    ;
      
      
      % Performs a raw assignment.
      % Given lvalue 2nd on stack, and expr value top of stack (already coerced to the lvalue type),
      % and type of both of these on the type stack,
      % perform the assignment and pop the type off the type stack.
      %
9883: Assign:
9885:    [ oTypeSNodeType
9887:       | nIntegerType, nEnumType :  .tAssignI
9890:       | nBooleanType, nByteType, nCharType :  .tAssignB
9894:       | nFileType :   #eNotImplemented
9898:       | nPointerType, nUniversalPointerType :  .tAssignP
            % compound var assigns; addrs of src, dest on stack
9902:       | nArrayType, nRecordType, nShortStringType :
                % TO DO: for ShortString, allow destination to be smaller capacity,
                % and truncate the copied value/length.
9904:           int size = oNodeGetInt( oTypeSTop, qSize )
9916:           .tCopy  oEmitInt( size )    % multi-word copy
9924:    ]
9950:    oTypeSPop
9952:    ;
      
      % Given nFunc for the just-accepted identifier on the left hand side.
      % Parse an assignment that represents assignment of the function result.
      %
9952: AssignResultStmt( Node funcDecl ):
         % Only allowed to assign the result of the immediately enclosing function, I think.
9954:    [ oNodeEqual( oNodeGet( funcDecl, qChildScope ), oScopeCurrent )
9970:       | false :   #eNotCurrentFunction
9973:       | * :
9978:    ]
      
         % Result value is stored in the caller's space, via a hidden VAR parameter.
9978:    .tPushParamP  oEmitInt( oNodeGetInt( funcDecl, qResultOffset ) )
9993:    oTypeSPush( oNodeGet( funcDecl, qType ) )
      
10006:    ':='
10008:    @Expr
10010:    @CoerceType
10012:    [ oTypeSNodeType
10014:       | nIntegerType, nEnumType : .tAssignI
10017:       | nBooleanType, nByteType, nCharType :  .tAssignB
10021:       | nFileType :   #eNotImplemented
10025:       | nPointerType, nUniversalPointerType :  .tAssignP
            % compound var assigns; addrs of src, dest on stack
10029:       | nArrayType, nRecordType, nShortStringType :
10031:           int size = oNodeGetInt( oTypeSTop, qSize )
10043:           .tCopy  oEmitInt( size )    % multi-word copy
10051:    ]
10077:    oTypeSPop
10079:    ;
      
      
      
      % Parse an lvalue expression, from scratch.
      % This is an expression that may appear on the left side of an assignment.
      % Leaves the address of the value on the expr stack,
      % and the type of the value at that address on the type stack.
      % If writeable is false, the expression won't be written to, so may access a
      % typed const.
      %
10079: LValueExpr( boolean writeable ):
         % At the moment, all lvalue expressions start with a variable name.
         % I think there can also be typecasts, not supported yet.  Check standard syntax.
10081:    pIdent
10083:    Node decl = oScopeFindRequire
10088:    @ResolveUnitRef( decl )
10095:    [ oNodeType( decl )
10102:       | nGlobalVar, nLocalVar, nParam, nTypedConst :
10103:       | * :  #eNotVar
10116:    ]
10116:    @LValueVar( decl, writeable )
10127:    ;
      
      
      % Parse an lvalue expression (e.g. left side of an assign statement)
      % that starts with the just-accepted identifier indicated by the given decl.
      % Leaves the lvalue address on the expression stack,
      % and the type of the value at that address on the type stack.
      %
10127: LValueVar( Node decl, boolean writeable ):
10129:    [ oNodeType( decl )
10136:       | nGlobalVar :   .tPushAddrGlobal @EmitValue( decl )
10146:       | nLocalVar :    .tPushAddrLocal @EmitValue( decl )
10157:       | nParam :
10159:          [ oNodeGetBoolean( decl, qInOut )
10169:             | true :   % VAR param points to variable.  No dereference.
10170:                        .tPushParamP @EmitValue( decl )
10179:             | * :      .tPushAddrParam @EmitValue( decl )
10193:          ]
10193:       | nTypedConst :
10195:          [ writeable
10198:             | false :  .tPushAddrGlobal @EmitValue( decl )                       
10208:             | * :      #eNotVar
10215:          ]
10215:       | * :            #eNotVar
10228:    ]
      
10228:    oTypeSPush( oNodeGet( decl, qType ) )
      
         % handle subscripts, if any.
         % There is one case that I'm not fitting into the pattern nicely, so need to
         % check for specially here:  if lvalue var is a pointer variable, and is
         % followed by [index], we need to dereference the pointer variable before applying
         % the index.  This is different from handling of [index] on an rvalue expression.
         % Right now I have LValueIndexes working for both lvalue and rvalue (composite types)
         % so can't easily add this behavior in there.
         % TO DO: probably I need separate rules LValueIndexes and RValueIndexes.
         %    This might also explain my confusion about handling of deref operator in LValueIndexes vs rvalue expr,
         %    and having separate rules might clear that up better.
      
10241:    [ oTypeSNodeType
10243:       | nPointerType :
10244:          [
10244:             | '[' :
                     % dereference the pointer var first
10246:                .tFetchP
10248:                @PointerArraySubscript
10250:             | * :
10255:          ]
10255:       | * :
10260:    ]
         % additional subscripts, if any
10260:    @LValueIndexes
10263:    ;
      
      
10263: IncVar( Node decl ):
10265:    @LValueVar( decl, true )
10275:    @RequireIntPop
10277:    @VarExpr( decl )
10284:    oTypeSPop
10285:    .tIncI
10287:    .tAssignI;
      
10290: DecVar( Node decl ):
10292:    @LValueVar( decl, true )
10302:    @RequireIntPop
10304:    @VarExpr( decl )
10311:    oTypeSPop
10312:    .tDecI
10314:    .tAssignI;
      
      
