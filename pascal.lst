6651:          @LValueIndexes
               % get final value of dereferencing, if any
6653:          @FetchVar
      
6655:       | pIdent :
6657:          Node decl = oScopeFindRequire
6662:          @ResolveUnitRef( decl )
6669:          Node theType
      
