11259: PointerTypeTo( Node theType ) >> Node:
11261:    Node ptrType = oNodeGet( theType, qPointerType )
11274:    [ oNodeNull( ptrType )
11281:       | true :
11282:          ptrType = oNodeNew( nPointerType )
11292:          oNodeSet( ptrType, qBaseType, theType )
11304:          oNodeSetInt( ptrType, qSize, 8 )
11316:          oTypeAdd( ptrType )
11322:          oNodeSet( theType, qPointerType, ptrType )
11334:       | * :
11339:    ]
