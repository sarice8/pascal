4502:          @EnumTypeRef( resultType )
      
      
            % *** TO DO: following type constructors are not implemented *** 
4509:       | pSet :
4511:          pOf
4513:          Node theType
4513:          @TypeRef( theType )
4520:       | * :       % this works for cases except where expr starts with an id
4535:          @ConstExpr '..' @ConstExpr
4541:    ];
      
      
