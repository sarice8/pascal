      % -------------------------------- Utilities -------------------------------
      
      % Return the static scope level of the current scope
      %
10969: ScopeLevel >> int:
10971:    >> oNodeGetInt( oScopeCurrent, qLevel );
      
      % Return the static scope level of the given declaration
      %
10981: DeclLevel( Node decl ) >> int:
10983:    >> oNodeGetInt( oNodeGet( decl, qParentScope ), qLevel );
         
      
      % How many levels higher is the given declaration from the current scope?
      %
11001: DeclUpLevels( Node decl ) >> int:
11003:    >> subtract( @ScopeLevel, @DeclLevel( decl ) );
      
      
      % Require that the top two types on the type stack are matching (ignoring subranges).
