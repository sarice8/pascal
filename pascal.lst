3915:                @LValueVar( decl, true )
3925:                @Expr
3927:                @CoerceType
3929:                @Assign
3931:                [ changedScope
3934:                   | true : oScopeEnd
3936:                   | * :
3941:                ]
3941:                oCodePop
                   
3942:             | * :
3947:          ]
      
