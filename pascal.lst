9549:          '('
9551:          @Expr
9553:          [ oTypeSNodeType
9555:             | nEnumType :
                     % TO DO: does this skip past type aliases?
                     % TO DO: what if using pred/succ on a subrange type?
9556:                [ oNodeGetBoolean( oTypeSTop, qHasGap )
9565:                   | true :   #eCantUsePredSuccOnEnumWithValueGaps
9568:                   | * :
9573:                ]
9573:             | * :  #eTypeMismatch
9580:          ]
9580:          .tDecI
9582:          ')'
9584:          >>
9585:       | * :
9590:    ]
      
         % Succ(x)
9590:    [ oNodeEqual( method, BuiltIn_Succ )
9600:       | true :
               % parameter is any enum type, as long as it has values 0..N-1
               % result is the same enum type
9601:          '('
9603:          @Expr
9605:          [ oTypeSNodeType
9607:             | nEnumType :
                     % TO DO: does this skip past type aliases?
                     % TO DO: what if using pred/succ on a subrange type?
9608:                [ oNodeGetBoolean( oTypeSTop, qHasGap )
9617:                   | true :   #eCantUsePredSuccOnEnumWithValueGaps
9620:                   | * :
9625:                ]
9625:             | * :  #eTypeMismatch
9632:          ]
9632:          .tIncI
