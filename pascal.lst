3731: VarDecl( node_type varNodeType ):
3733:    {[
3733:       | pIdent :
3735:          NodeVec decls = oNodeVecNew
3740:          Node decl
3740:          {
3740:             decl = @newIdent( varNodeType, LAST_ID )
3753:             oNodeVecAppend( decls, decl )
3762:             [
3762:                | ',' :
3764:                   pIdent
3766:                | * :
3771:                   >
3773:             ]
3773:          }
3775:          ':'
3777:          Node theType
3777:          @TypeRef( theType )
      
3784:          int i = 0
3790:          {[ equal( i, oNodeVecSize( decls ) )
3804:             | false :
