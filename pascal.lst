      %
11019: MatchTypes:
11021:    node_type nt = oTypeSNodeType
11026:    oTypeSPop
11027:    [ equal_node_type( nt, oTypeSNodeType )
11036:       | false :
               % Some implicit conversion is allowed even here
11037:          [ oTypeSNodeType
11039:             | nPointerType :
11040:                [ nt
11043:                   | nUniversalPointerType :  >>
11045:                   | * :
11050:                ]
11050:             | nUniversalPointerType :
11052:                [ nt
11055:                   | nPointerType :  >>
11057:                   | * :
11062:                ]
11062:             | * :
11069:          ]
11069:          #eTypeMismatch
11071:       | * :
11076:    ];
      
      
      % Require that the top type on the type stack is integer (ignoring subranges).
      % Pop it.
      %
11077: RequireIntPop:
11079:    [ oTypeSNodeType
11081:       | nIntegerType :
11082:       | * :          #eNotInteger
11089:    ]
11089:    oTypeSPop;
      
11091: RequireInt:
11093:    [ oTypeSNodeType
11095:       | nIntegerType :
11096:       | * :          #eNotInteger
11103:    ];
      
      
