1945: MethodModifiers( Node decl ):
1947:    {[
1947:       | pCdecl :
1949:          oNodeSetBoolean( decl, qCdecl, true )
1961:          ';'
      
1963:       | * : >
1970:    ]}
1973:    ;
      
      
